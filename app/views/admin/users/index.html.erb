<h1>Listing users</h1>

<table>
  <tr>
    <th>Id</th>
    <th>Name</th>
    <th>Display name</th>
    <th>Email</th>
    <th>Open identifier</th>
    <th>Password</th>
    <th>Reputation</th>
    <th>Is banned</th>
    <th>Last seen on</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.id %></td>
    <td><%= user.name %></td>
    <td><%= user.display_name %></td>
    <td><%= user.email %></td>
    <td><%= user.open_identifier %></td>
    <td><%= user.password %></td>
    <td><%= user.reputation %></td>
    <td><%= user.is_banned %></td>
    <td><%= user.last_seen_on %></td>
    <td><%= link_to 'Show', admin_user_path(user) %></td>
    <td><%= link_to 'Edit', edit_admin_user_path(user) %></td>
    <td><%= link_to 'Destroy', admin_user_path(user), confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_admin_user_path %>
